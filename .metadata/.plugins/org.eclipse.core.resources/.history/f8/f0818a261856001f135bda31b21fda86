package Assignment_09_08_2024.com;

public class Pattern_Q3 
{
	public static void main(String[] args) 
	{
		int ch = 65;
		int i,j;
		for(i = 1 ; i <= 5 ; i++)
		{
			for(j = 1 ; j <= 5 ; j++)
			{
				if(i<=5 || j<=5)
				{
					System.out.printf("%c " , ch++);
				}
			}
			ch=ch-j;
			System.out.println("");
		}

	}

}

package Assignment_09_08_2024.com;

public class Pattern_Q3I 
{
	public static void main(String[] args) 
	{
		for(int i = 0 ; i < 5 ; i++)
		{
			int ch = 65;
			
			for(int j = 0 ; j <5 ; j++)
			{
				ch = ch + i;
				System.out.printf("%c " , ch);
				ch++;
			}
			System.out.println("");
		}

	}

}

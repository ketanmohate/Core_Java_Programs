package Assignment_09_08_2024.com;

public class Pattern_Q3 
{
	public static void main(String[] args) 
	{
		for(int i = 1 ; i <= 5 ; i++)
		{
			char ch = (char)('A' + i);
			
			for(int j = 1 ; j <= 5 ; j++)
			{
				System.out.print(ch + " ");
				ch++;
			}
			System.out.println("");
		}

	}

}

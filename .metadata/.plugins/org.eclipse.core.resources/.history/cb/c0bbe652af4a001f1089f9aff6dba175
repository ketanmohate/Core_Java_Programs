package com.java;

/* 
Pattern
 
   

End
*/

public class Pattern_Q37 
{
	public static void main(String[] args) 
	{	
		int count=1;
		int count1=1;
		
		for(int i = 1 ; i<=5 ; i++)
		{
			for(int j = 1 ; j<=10 ; j++)
			{
				if(j<=5)
				{
					System.out.print(count);					}
				else
				{
					System.out.print(count1);
				}
			}
			}
			System.out.println(" ");
		}
	}
}

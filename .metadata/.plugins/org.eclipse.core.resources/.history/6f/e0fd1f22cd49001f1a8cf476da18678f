package com.java;

/* 
Pattern
 
             

End
*/

public class Pattern_Q16 
{
	public static void main(String[] args)
	{		
		for(int i = 1 ; i <= 5 ; i++)
		{	
			int count2=1;
			int count=1;
			for(int j = 1 ; j<=9 ; j++)
			{
				if((j<=5 && j>=6-i))
				{
					System.out.print(count + "  ");
					count++;
				}
				else if((j>=6 && j<=4+i))
				{
					System.out.print(count2 + "  ");
					count2--;
				}
				else
				{
					System.out.print("   ");
				}
			}
			count2++;
			System.out.println("");
		}
	}
}

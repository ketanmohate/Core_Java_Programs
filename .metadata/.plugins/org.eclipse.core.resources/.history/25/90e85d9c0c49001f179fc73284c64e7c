package com.java;

public class Pattern_Q12 
{
	public static void main(String[] args)
	{
		int count=1;
		
		for(int i = 1 ; i <= 5 ; i++)
		{	
			for(int j = 1 ; j<=9 ; j++)
			{
				if((j<=5 && j>=6-i)||(j>=6 && j<=4+i))
				{
					System.out.print(i + " ");
				}
				else
				{
					System.out.print("  ");
				}
			}
			System.out.println("");
		}
	}
	//if(j<=5 && j>=6-i)

}

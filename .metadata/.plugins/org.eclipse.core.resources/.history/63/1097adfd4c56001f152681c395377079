package Assignment_09_08_2024.com;

public class Pattern_Q3 
{
	public static void main(String[] args) 
	{
		for(int i = 0 ; i < 5 ; i++)
		{
			char startChar = (char) ('A' + i);
			
			for(int j = 0 ; j < 5 ; j++)
			{
				System.out.print(startChar + " ");
				startChar++;
			}
			System.out.println("");
		}

	}

}

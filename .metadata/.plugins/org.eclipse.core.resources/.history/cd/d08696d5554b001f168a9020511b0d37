public class Pattern_Q40
{
	public static void main(String[] args) 
	{			
		for(int i = 1 ; i<=5 ; i++)
		{	
			char ch='A';			
			for(int j = 1 ; j<=9 ; j++)
			{
				if(j<=6-i)
				{
					System.out.print(ch + " ");
					ch++;
				}
				else if(j>=4+i)
				{
					System.out.print(chh + " ");
					--chh;
				}
				else
				{
					System.out.print("  ");
				}
			}
			System.out.println(" ");
		}
	}
}
package com.java;

/* 
Pattern
 
             

End
*/

public class Pattern_Q15 
{
	public static void main(String[] args)
	{			
		int count=1;
		
		for(int i = 1 ; i <= 5 ; i++)
		{	
			for(int j = 1 ; j<=9 ; j++)
			{
				if(j<=i+1)
				{
					System.out.print("* ");
				}
				else
				{
					System.out.print("   ");
				}
			}
			System.out.println("");
		}
	}
}

package Loopng_PDF;

import java.util.*;

public class Q15_Calculate_Product_Of_Digit 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Enter the Number");
		int num = sc.nextInt();
		
		int rem = 0;
		for(;num!=0;)
		{
			rem = num % 10;
			
		}
	}

}

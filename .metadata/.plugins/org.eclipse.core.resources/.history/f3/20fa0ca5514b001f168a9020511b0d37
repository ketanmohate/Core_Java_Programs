package com.java;

/* 
Pattern
 
 

End
*/

public class Pattern_Q38 
{
	public static void main(String[] args) 
	{	
		for(int i = 1 ; i<=5 ; i++)
		{
			int count=1;
			
			for(int j = 1 ; j<=9 ; j++)
			{
				if(j<=9-i*2 && j<5)
				{
					System.out.print(count);
					count++;
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println(" ");
		}
	}
}
*/

public class Pattern_Q40
{
	public static void main(String[] args) 
	{			
		for(int i = 1 ; i<=5 ; i++)
		{	
			int ch = 65;
			int chh = 69;
			
			for(int j = 1 ; j<=9 ; j++)
			{
				if(j<=6-i)
				{
					System.out.printf("%c" , ch);
					ch++;
				}
				else
				{
					System.out.print("  ");
				}
			}
			System.out.println(" ");
		}
	}
}
package com.java;

public class Pattern_Q12 
{
	public static void main(String[] args)
	{
		for(int i = 1 ; i <= 5 ; i++)
		{
			int count=1;
			
			for(int j = 1 ; j<=5 ; j++)
			{
				if(j<=5 && j>=6-i)
				{
					System.out.print(j + " ");
					count++;
				}
				else
				{
					System.out.print("  ");
				}
			}
		}
	}
	//if(j<=5 && j>=6-i)

}

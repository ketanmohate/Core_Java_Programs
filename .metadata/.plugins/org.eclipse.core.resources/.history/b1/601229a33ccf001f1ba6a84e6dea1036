//			area[i] = computeAreaSquare(a , b, c);
		}
		
		double sum=0;
		
		for(int i=0;i<arr.length;i++)
		{
			for(int j=0;j<arr[i].length;i++)
			{
				
			}
		}
		
		System.out.println("print");
		for(int i=0 ; i<arr.length ; i++)

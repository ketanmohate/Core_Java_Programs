package com.java;

/* 
 pattern 
 
        1
      2 3 2 
    3 4 5 4 3
  4 5 6 7 6 5 4
5 6 7 8 9 8 7 6 5

end
 */

public class Pattern_Q12
{
	public static void main(String[] args)
	{		
		for(int i = 1 ; i <= 5 ; i++)
		{	
			for(int j = 1 ; j<=9 ; j++)
			{
				if((j<=5 && j>=6-i))
				{
					System.out.print(i + " ");
				}
				else
				{
					System.out.print("  ");
				}
			}
			System.out.println("");
		}
	}
	
	//||(j>=6 && j<=4+i)

}

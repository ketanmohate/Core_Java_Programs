package com.java;

import javax.swing.plaf.synth.SynthSliderUI;

/* 
Pattern
 
A
ABA
ABCBA
ABCDCBA
ABCDEDCBA

End
*/

public class Pattern_Q42
{
	public static void main(String[] args) 
	{			
		for(int i = 1 ; i<=5 ; i++)
		{	
			int ch=65;
			int chh=65;
			
			for(int j = 1 ; j<=9 ; j++)
			{
				if(j<=i)
				{
					System.out.printf("%c ", ch);
					ch++;
				}
				else if(j<=i*2-i)
				{
					System.out.printf("%c ", chh);
				}
			}
			System.out.println(" ");
		}
	}
}
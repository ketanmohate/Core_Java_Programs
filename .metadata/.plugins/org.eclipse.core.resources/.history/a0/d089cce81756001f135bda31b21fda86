package Assignment_09_08_2024.com;

public class Pattern_Q3 
{
	public static void main(String[] args) 
	{
		for(int i = 1 ; i <= 5 ; i++)
		{
			int ch = 65;
			
			for(int j = 1 ; j <= 5 ; j++)
			{
				if(i<=5 || j<=5)
				{
					System.out.printf("%c " , ch++);
				}
			}
			System.out.println("");
		}

	}

}

package com.java;

/* 
Pattern
 


End
*/

public class Pattern_Q27
{
	public static void main(String[] args)
	{		
		for(int i = 1 ; i <=5 ; i++)
		{	
			for(int j = 1 ; j<=10 ; j++)
			{	
				if()
				{
					System.out.print("* ");
				}
				else 
				{
					System.out.print(j+" ");
				}
			}
			System.out.println("");
		}
	}
}
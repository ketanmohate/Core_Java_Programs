package com.java;

public class Pattern_Q9 
{
	public static void main(String[] args) 
	{	
		for(int j = 1 ; j <=11 ; j++)
		{
			if((i==7-j || i>=6 && flag==0) || (j>=6 && (j>=7 && j==5+i & flag==0)))
			{
				System.out.print(" * ");
				flag=1;
			}
			else
			{
				System.out.print("   ");
				flag=0;
			}
		}
		System.out.println("");
	}
	}

}

package com.java;

public class H_Patteen 
{
	public static void main(String[] args)
	{
		int n=6;
		for(int i = 1 ; i <=n ; i++)
		{
			for(int j = 1 ; j <=n ; j++)
			{
				if(i == n - 1 || j == 0 || j == i)
				{
					System.out.print("* ");
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println("");
		}
	}

}

package com.java;

/* 
Pattern
 
123454321
1234321
12321
121
1 

End
*/

public class Pattern_Q38 
{
	public static void main(String[] args) 
	{	
		for(int i = 1 ; i<=5 ; i++)
		{
			int count=1;
			
			for(int j = 1 ; j<=9 ; j++)
			{
				if((j<=6-i*2))
				{
					System.out.print(count);
					count++;
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println(" ");
		}
	}
}
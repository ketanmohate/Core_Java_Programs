package com.java;

/* 
Pattern
 
 

End
*/

public class Pattern_Q39
{
	public static void main(String[] args) 
	{	
		for(int i = 1 ; i<=5 ; i++)
		{
			char ch='a';
			
			for(int j = 1 ; j<=5 ; j++)
			{
				if(i>=j)
				{
					System.out.print(ch++);
				}
				else
				{
					System.out.print("  ");
				}
			}
			System.out.println(" ");
		}
	}
}
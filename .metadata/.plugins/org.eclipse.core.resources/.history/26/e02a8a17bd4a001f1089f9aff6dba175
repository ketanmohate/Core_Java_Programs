package com.java;

/* 
Pattern
 
   

End
*/

public class Pattern_Q37 
{
	public static void main(String[] args) 
	{	
		for(int i = 1 ; i<=5 ; i++)
		{
			int count=1;
			
			for(int j = 1 ; j<=10 ; j++)
			{
				if(j<=5 && i>=j)
				{
					System.out.print(count);
					count++;
				}
				else
				{
					System.out.print(" ");
				}
			}
		}
	}
}

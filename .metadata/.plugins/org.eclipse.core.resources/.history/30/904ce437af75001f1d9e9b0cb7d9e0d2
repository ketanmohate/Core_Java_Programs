package Assignment_04_09_2024.java;

import java.util.*;

class Player
{
	private int id; 
	private String name; 
	private int runs; 
	
	public void setId(int id) 
	{ 
		this.id=id; 
	} 
	public int getId() 
	{ 
		return id; 
	} 
	
	public void setName(String name) 
	{ 
		this.name=name; 
	} 
	public String getName() 
	{ 
		return name; 
	} 
	
	public void setRuns(int runs) 
	{ 
		this.runs=runs; 
	} 
	public int getRuns() 
	{ 
		return runs; 
	} 
}


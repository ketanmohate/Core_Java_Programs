package com.java;

public class H_Patteen 
{
	public static void main(String[] args)
	{
		int n=6;
		for(int i = 1 ; i <=n ; i++)
		{
			for(int j = 1 ; j <=n ; j++)
			{
				if()
				{
					System.out.print(" * ");
				}
				else
				{
					System.out.print("   ");
				}
			}
			System.out.println("");
		}
	}

}

public class Pattern_Q41
{
	public static void main(String[] args) 
	{			
		for(int i = 1 ; i<=5 ; i++)
		{	
			int ch=64;
			int chh=69;
			
			for(int j = 1 ; j<=9 ; j++)
			{
				if()
				{
					System.out.printf("%c" , ch+j);
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println(" ");
		}
	}
}
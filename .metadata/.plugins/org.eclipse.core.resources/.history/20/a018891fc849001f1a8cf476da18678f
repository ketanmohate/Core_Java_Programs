package com.java;

/* 
Pattern
 
             

End
*/

public class Pattern_Q15 
{
	public static void main(String[] args)
	{	
		char  ch =64;
		
		for(int i = 1 ; i <= 5 ; i++)
		{	
			for(int j = 1 ; j<=9 ; j++)
			{
				if(j<=i*2-1)
				{
					System.out.printf("%c" + ch+i);
				}
				else
				{
					System.out.print("   ");
				}
			}
			System.out.println("");
		}
	}
}

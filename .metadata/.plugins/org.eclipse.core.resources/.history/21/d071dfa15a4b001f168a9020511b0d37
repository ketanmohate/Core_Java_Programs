public class Pattern_Q40
{
	public static void main(String[] args) 
	{			
		for(int i = 1 ; i<=5 ; i++)
		{	
			int ch=64;
			int chh=79;
			
			for(int j = 1 ; j<=9 ; j++)
			{
				if(j<=6-i)
				{
					System.out.printf("%c" , ch+i);
				}
//				else if(j>=4+i)
//				{
//					System.out.print(chh + " ");
//				}
				else
				{
					System.out.print("  ");
				}
			}
			System.out.println(" ");
		}
	}
}
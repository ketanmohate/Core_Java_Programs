package com.java;

/* 
Pattern
 
             

End
*/

public class Pattern_Q16 
{
	public static void main(String[] args)
	{		
		for(int i = 1 ; i <= 5 ; i++)
		{	
			for(int j = 1 ; j<=17 ; j++)
			{
				if((j<=9 && j==10-i)||(j>=10))
				{
					System.out.print(" " + i + " ");
				}
				else
				{
					System.out.print(" * ");
				}
			}
			System.out.println("");
		}
	}
}

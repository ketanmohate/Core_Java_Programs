package Assignment_04_09_2024.java;

import java.util.*;

class Product
{
	private int id; 
	private String name; 
	private int qty; 
	private int rate;
	
	public void setId(int id)
	{
		this.id=id;
	}
	public int getId()
	{
		return id;
	}
	
	public void setName(String name)
	{
		this.name=name;
	}
	public String getName()
	{
		return name;
	}
	
	public void setQTY(int qty)
	{
		this.qty=qty;
	}
	public int getQTY()
	{
		return qty;
	}
	
	public void setRate(int rate)
	{
		this.rate=rate;
	}
	public int getRate()
	{
		return rate;
	}
}
public class Q11_Billing_App 
{
	public static void main(String[] args) 
	{
		
	}

}

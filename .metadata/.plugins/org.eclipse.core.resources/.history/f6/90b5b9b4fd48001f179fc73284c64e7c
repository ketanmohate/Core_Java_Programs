package com.java;

public class Pattern_Q9 
{
	public static void main(String[] args) 
	{
		for(int i = 1 ; i <= 6 ; i++)
		{	
			for(int j = 1 ; j <=11 ; j++)
			{
				if()
				{
					System.out.print(" * ");
				}
				else
				{
					System.out.print("   ");
				}
			}
			System.out.println("");
		}
	}

}
